{
  "name": "boilerplate-react-js",
  "version": "0.0.1",
  "description": "boilerplate-react-js - App",
  "author": "BBS",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "prepare": "husky install && rm -rf $(git rev-parse --git-dir)/hooks && ln -s ../.husky $(git rev-parse --git-dir)/hooks",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "generate": "graphql-codegen -r dotenv/config -c codegen.yml",
    "generate:contentful": "graphql-codegen -r dotenv/config -c codegen-contentful.yml",
    "storybook": "NODE_ENV=test start-storybook --static-dir ./public --port 49999",
    "storybook:build": "NODE_ENV=test build-storybook --config-dir .storybook --output-dir storybook_public/ --static-dir ./public"
  },
  "browserslist": [
    "> 0.25%",
    "ie >= 11",
    "not op_mini all",
    "not dead"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,md,html,css}": "prettier --write"
  },
